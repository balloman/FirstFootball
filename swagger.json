{
  "openapi": "3.0.1",
  "info": {
    "title": "FirstFootball.Backend",
    "version": "v1"
  },
  "paths": {
    "/fixtures": {
      "get": {
        "tags": [
          "GetFixtures"
        ],
        "operationId": "GetFixtures",
        "parameters": [
          {
            "name": "limit",
            "in": "query",
            "style": "form",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "beforeMs",
            "in": "query",
            "style": "form",
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Fixture"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Fixture": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "homeTeam": {
            "$ref": "#/components/schemas/Team"
          },
          "homeTeamName": {
            "type": "string",
            "nullable": true
          },
          "awayTeam": {
            "$ref": "#/components/schemas/Team"
          },
          "awayTeamName": {
            "type": "string",
            "nullable": true
          },
          "homeScore": {
            "type": "integer",
            "format": "int32"
          },
          "awayScore": {
            "type": "integer",
            "format": "int32"
          },
          "matchWeek": {
            "type": "integer",
            "format": "int32"
          },
          "datePosted": {
            "type": "string",
            "format": "date-time",
            "example": "2023-04-14T04:45:45.790242Z"
          }
        },
        "additionalProperties": false
      },
      "Team": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "homeFixtures": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Fixture"
            },
            "nullable": true
          },
          "awayFixtures": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Fixture"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}